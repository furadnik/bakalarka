@article{doi:10.1287/ijoc.15.3.284.16077,
	abstract = { Many auctions involve the sale of a variety of distinct assets.
	            Examples are airport time slots, delivery routes, network routing,
	            and furniture. Because of complementarities or substitution effects
	            between the different assets, bidders have preferences not just for
	            particular items but for sets of items. For this reason, economic
	            efficiency is enhanced if bidders are allowed to bid on bundles or
	            combinations of different assets. This paper surveys the state of
	            knowledge about the design of combinatorial auctions and presents
	            some new insights. Periodic updates of portions of this survey will
	            be posted to this journal's Online Supplements web page at
	            http://joc.pubs.informs.org/OnlineSupplements.html },
	author = {de Vries, Sven and Vohra, Rakesh V.},
	doi = {10.1287/ijoc.15.3.284.16077},
	eprint = { https://doi.org/10.1287/ijoc.15.3.284.16077 },
	journal = {INFORMS Journal on Computing},
	number = {3},
